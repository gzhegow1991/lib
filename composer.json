{
    "name": "gzhegow/lib",
    "type": "library",
    "version": "1.2.0",
    "license": "MIT",

    "authors": [
        {
            "name": "gzhegow",
            "email": "6562680@gmail.com"
        }
    ],

    "minimum-stability": "dev",
    "prefer-stable": true,

    "autoload": {
        "psr-4": {
            "Gzhegow\\Lib\\": "src/"
        },
        "files": [
            "const/arr.php",
            "const/cmp.php",
            "const/debug.php",
            "const/num.php",
            "const/php.php",
            "const/type.php"
        ],
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Gzhegow\\Lib\\Tests\\": "tests/src/"
        }
    },

    "require": {
        "php": "^7.3|^8.0",

        "ext-pcre": "*"
    },
    "suggest": {
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-filter": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-memcached": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "ext-simplexml": "*",
        "ext-sockets": "*",

        "doctrine/inflector": "^2.0",

        "symfony/http-foundation": "^5.0",
        "symfony/process": "^5.0",
        "symfony/string": "^5.0",
        "symfony/var-dumper": "^5.0",

        "symfony/translation-contracts": "^2.0",

        "egulias/email-validator": "^3.0",

        "giggsey/libphonenumber-for-php": "^8.0"
    },

    "config": {
        "optimize-autoloader": true,

        "sort-packages": false
    }
}